@model List<Doctor>

<html>
	<head>
		<title>Doctor list Page</title>
		<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
	</head>
	<body>
		<h1> Doctor index Page</h1>
		<table class="table	">
			<thead>
				<tr>
					<th>Id</th>
					<th>Name</th>
					<th>Qualification</th>
					<th>Action</th>
				<th>Action</th>
				</tr>
			</thead>
			<tbody>
				@{
					foreach (Doctor doctor in Model)
					{
						<tr>
					<td>@doctor.Id</td>
					<td>@doctor.Name</td>
					<td>@doctor.Qualification</td>
					<td><a href="/Doctor/Delete/@doctor.Id"> Delete</a></td>
						<td><a href="/Doctor/Update/@doctor.Id"> Update</a></td>
					</tr>
					}
				}
			</tbody>
		</table>
		<a href="/Doctor/Create">Add new Doctor</a>
	</body>
</html>